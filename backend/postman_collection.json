            },
            "url": {
              "raw": "{{baseUrl}}/users/login",
              "host": ["{{baseUrl}}"],
              "path": ["users", "login"]
            }
          },
          "response": [],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Store session cookie for subsequent requests",
                  "if (pm.response.code === 200) {",
                  "    const setCookie = pm.response.headers.get('Set-Cookie');",
                  "    if (setCookie) {",
                  "        pm.globals.set('sessionCookie', setCookie);",
                  "        pm.collectionVariables.set('sessionCookie', setCookie);",
                  "    }",
                  "}",
                  "",
                  "pm.test('Login successful', function () {",
                  "    pm.response.to.have.status(200);",
                  "    pm.expect(pm.response.json().success).to.be.true;",
                  "    pm.expect(pm.response.json().data.user).to.have.property('id');",
                  "    pm.expect(pm.response.json().data.user).to.have.property('email');",
                  "});"
